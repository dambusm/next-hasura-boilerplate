{
  "name": "@next-hasura-boilerplate/app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "start": "concurrently \"vercel dev\" \"yarn graphql-codegen --watch\"",
    "deploy:dev": "vercel",
    "deploy:prod": "vercel --prod",
    "introspect": "source '.env' && gq ${HASURA_GRAPHQL_URL} -H \"x-hasura-admin-secret: ${HASURA_ADMIN_SECRET}\" --introspect > src/graphql/schema.graphql",
    "graphql-codegen": "yarn introspect && graphql-codegen --config src/graphql/codegen.yml"
  },
  "dependencies": {
    "graphql": "15.4.0",
    "graphql-request": "3.3.0",
    "next": "10.0.0",
    "next-auth": "3.1.0",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.19.0",
    "@graphql-codegen/typescript": "1.17.11",
    "@graphql-codegen/typescript-compatibility": "2.0.0",
    "@graphql-codegen/typescript-graphql-request": "2.0.2",
    "@graphql-codegen/typescript-operations": "1.17.8",
    "@now/node": "1.8.4",
    "@types/next-auth": "3.1.17",
    "@types/react": "16.9.55",
    "concurrently": "5.3.0",
    "graphqurl": "0.3.3",
    "vercel": "21.0.1"
  },
  "peerDependencies": {
    "prettier": "*",
    "typescript": "*"
  }
}
